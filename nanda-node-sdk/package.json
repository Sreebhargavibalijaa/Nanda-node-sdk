{
  "name": "nanda-node-sdk",
  "version": "1.0.0",
  "description": "Node.js SDK for NANDA Base Agent - Customizable AI Agent Communication System",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "example": "ts-node examples/basic-agent.ts",
    "example:custom": "ts-node examples/custom-improvement.ts",
    "example:server": "ts-node examples/server-mode.ts"
  },
  "keywords": [
    "nanda",
    "ai",
    "agent",
    "sdk",
    "typescript",
    "nodejs",
    "anthropic",
    "claude",
    "a2a"
  ],
  "author": "NANDA Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nanda-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/nanda-node-sdk/issues"
  },
  "homepage": "https://github.com/yourusername/nanda-node-sdk#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/ws": "^8.5.8",
    "@types/uuid": "^9.0.6",
    "@types/jest": "^29.5.6",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
} 